{
  "name": "@kleros/kleros-sdk",
  "version": "2.0.1-2",
  "description": "SDK for Kleros version 2",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kleros/kleros-v2.git"
  },
  "author": "Kleros",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "module": "./lib/index.js",
  "files": [
    "lib/**/*",
    "!lib/**/test/*"
  ],
  "alias": {
    "src": "./src",
    "dataMappings": "./src/dataMappings"
  },
  "packageManager": "yarn@4.0.2+sha256.825003a0f561ad09a3b1ac4a3b3ea6207af2796d54f62a9420520915721f5186",
  "engines": {
    "node": ">=16.0.0"
  },
  "type": "commonjs",
  "volta": {
    "node": "20.11.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn clean && tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "publish:dev": "yarn npm publish --tag dev"
  },
  "devDependencies": {
    "@types/mustache": "^4.2.5",
    "@vitest/ui": "^1.1.3",
    "dotenv": "^16.3.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3"
  },
  "dependencies": {
    "@kleros/kleros-v2-contracts": "workspace:^",
    "@reality.eth/reality-eth-lib": "^3.2.30",
    "mustache": "^4.2.0",
    "zod": "^3.22.4"
